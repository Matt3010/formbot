# FormBot Environment Configuration

# Laravel
APP_KEY=base64:GENERATE_WITH_php_artisan_key_generate
APP_DEBUG=true
APP_URL=http://localhost:8000

# Database
DB_PASSWORD=formbot_secret_password

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Pusher/Soketi WebSocket
PUSHER_APP_ID=formbot
PUSHER_APP_KEY=formbot-key
PUSHER_APP_SECRET=formbot-secret
PUSHER_HOST=websocket
PUSHER_PORT=6001
PUSHER_SCHEME=http

# Encryption key for Python service (base64-encoded 32-byte key)
ENCRYPTION_KEY=GENERATE_A_BASE64_32BYTE_KEY

# Ollama
OLLAMA_URL=http://ollama:11434
OLLAMA_MODEL=llama3.1:8b

# Scraper
SCRAPER_SERVICE_URL=http://scraper:9000
SCRAPER_TIMEOUT=600

# FormBot Defaults
FORMBOT_RETENTION_DAYS=30
FORMBOT_DEFAULT_DELAY=500
FORMBOT_MAX_PARALLEL=5

# VNC
VNC_ENABLED=true
